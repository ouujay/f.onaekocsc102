   "source": [
    "# function\n",
    "def emojiconverter(message):\n",
    "    words = message.split(' ')\n",
    "    emoji = {\n",
    "    \":)\":\"😁\",\n",
    "    \":(\":\"😒\"\n",
    "    }  \n",
    "    output = \"\"\n",
    "    for word in words:\n",
    "        output += emoji.get(word, word) + \" \"   \n",
    "    return output\n",
    "\n",
    "\n",
    "message = input(\">\")\n",
    "print(emojiconverter(message))\n"
   ]
  }
